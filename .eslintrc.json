{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "fetch": true
  },
  "rules": {
    "indent": [1, 2, {
        "SwitchCase": 1
    }],
    "semi": [2, "always"],
    "space-unary-ops": 2,
    "curly": 2,
    "eqeqeq": 2
  }
}
